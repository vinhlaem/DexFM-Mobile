# ============================================
# Dex.FM Mobile - Environment Variables
# ============================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# ============================================
# Alchemy API Configuration
# ============================================
# Get your API keys from: https://dashboard.alchemy.com/
EXPO_PUBLIC_ALCHEMY_ETH_KEY=your_alchemy_ethereum_api_key_here
EXPO_PUBLIC_ALCHEMY_ETH_URL=https://eth-sepolia.g.alchemy.com/v2/
EXPO_PUBLIC_ALCHEMY_SOCKET_URL=wss://eth-sepolia.g.alchemy.com/v2/
EXPO_PUBLIC_ALCHEMY_SOL_URL=https://solana-devnet.g.alchemy.com/v2/
EXPO_PUBLIC_ALCHEMY_SOL_API_KEY=your_alchemy_solana_api_key_here

# ============================================
# Environment
# ============================================
# Options: "development" | "production"
EXPO_PUBLIC_ENVIRONMENT=development

# ============================================
# API Endpoints
# ============================================
# Backend API Base URL
EXPO_PUBLIC_BASE_URL=https://dexfmbe.azurewebsites.net/v1

# DexScreener API (public, no key required)
EXPO_PUBLIC_DEXSCREENER_API=https://api.dexscreener.com

# GeckoTerminal API (public, no key required)
EXPO_PUBLIC_GECKOTERMINAL_API=https://api.geckoterminal.com

# GoPlusLabs API (public, no key required)
EXPO_PUBLIC_GOPLUSLABS_API=https://api.gopluslabs.io
